{
  "name": "mapify-backend",
  "version": "1.0.0",
  "description": "Backend for Mapify with fully automatic OpenStreetMap importer and MongoDB integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "import": "node importer/autoImporter.js",
    "scrape": "node scripts/scrape.js",
    "scrape:zomato": "node scripts/scrape.js --platform=zomato",
    "scrape:swiggy": "node scripts/scrape.js --platform=swiggy",
    "scrape:magicpin": "node scripts/scrape.js --platform=magicpin",
    "scrape:district": "node scripts/scrape.js --platform=district",
    "seed:places": "node scripts/seedPlaces.js",
    "clear:seed": "node scripts/clearSeedData.js",
    "discover:places": "node scripts/discoverPlaces.js"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "mongoose": "^9.0.1",
    "node-cron": "^4.2.1",
    "puppeteer": "^24.34.0",
    "string-similarity": "^4.0.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
